generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = "postgresql://edimario:edimario@localhost:5432/slim_chat"
}

model User {
    id Int @id @default(autoincrement()) 
    name String
    email String @unique
    password String
    profilePhoto String
    createdAt DateTime @default(now())
    updateAt DateTime @updatedAt 
}